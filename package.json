{
  "name": "kremen-deputies",
  "version": "2.0.0",
  "title": "Карта виборчих округів м. Кременчук",
  "description": "Карта виборчих округів дозволяє вам дізнатись хто є депутатом вашого району та як з ним зв'язатись",
  "company": "IQ Hub",
  "url": "https://deputat.io.kr.ua/",
  "main": "src/index.tsx",
  "scripts": {
    "start": "yarn start:dev",
    "start:dev": "webpack-dev-server --mode development --env.ENV=dev",
    "start:prod": "webpack-dev-server --mode development --env.ENV=prod",
    "dev": "webpack -d --watch --env.ENV=dev --progress --display=detailed",
    "dist": "yarn dist:dev",
    "dist:dev": "rm -rf dist/* && webpack -p --progress --env.ENV=dev",
    "dist:prod": "rm -rf dist/* && webpack -p --progress --env.ENV=prod",
    "lint:types": "tsc --noEmit --skipLibCheck",
    "lint:code": "tslint -p tsconfig.json -e node_modules -e dist --fix",
    "lint": "yarn lint:types && yarn lint:code",
    "deploy": "firebase deploy --only hosting"
  },
  "author": "Jaroslav Khorishchenko <websnipter@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "4.4.0",
    "@material-ui/icons": "4.4.1",
    "@material-ui/styles": "4.3.3",
    "axios": "0.19.0",
    "color": "3.1.2",
    "lodash": "4.17.15",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-google-maps": "9.4.5",
    "react-places-autocomplete": "7.2.1",
    "react-router-dom": "5.0.1"
  },
  "devDependencies": {
    "@types/googlemaps": "3.37.6",
    "@types/lodash": "4.14.138",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-places-autocomplete": "7.2.5",
    "@types/react-router-dom": "4.3.5",
    "babel-loader": "8.0.6",
    "copy-webpack-plugin": "5.0.4",
    "css-loader": "3.2.0",
    "dotenv": "8.1.0",
    "file-loader": "4.2.0",
    "html-webpack-plugin": "3.2.0",
    "node-sass": "4.12.0",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.0",
    "ts-loader": "6.0.4",
    "tslint": "5.19.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "4.0.0",
    "typescript": "3.6.2",
    "typescript-tslint-plugin": "0.5.4",
    "url-loader": "2.1.0",
    "webpack": "4.39.3",
    "webpack-cli": "3.3.7",
    "webpack-dev-server": "3.8.0"
  }
